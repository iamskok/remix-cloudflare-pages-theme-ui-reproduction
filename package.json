{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "remix build",
    "deploy": "yarn build && wrangler publish",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public --compatibility-date=2022-10-22",
    "dev": "remix build && run-p \"dev:*\"",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "postinstall": "remix-esbuild-override"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.3",
    "@emotion/react": "^11.10.4",
    "@emotion/server": "^11.10.0",
    "@remix-run/cloudflare": "^1.7.3",
    "@remix-run/cloudflare-pages": "^1.7.3",
    "@remix-run/react": "^1.7.3",
    "@theme-ui/core": "^0.15.3",
    "cross-env": "^7.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "theme-ui": "^0.15.3"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@remix-run/dev": "^1.7.3",
    "@remix-run/eslint-config": "^1.7.3",
    "@types/react": "^17.0.47",
    "@types/react-dom": "^17.0.17",
    "eslint": "^8.23.1",
    "npm-run-all": "^4.1.5",
    "remix-esbuild-override": "^3.0.4",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.27"
  },
  "engines": {
    "node": ">=16.13"
  }
}
